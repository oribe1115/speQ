{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "fmt": "prettier --write --ignore-path ./.prettierignore src/",
    "fmt:check": "prettier --check --ignore-path ./.prettierignore src/",
    "lint": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "openapi-gen": "openapi --input ../schema/api/openapi.yaml --output src/apis/generated --client fetch --name AppClient"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.2.2",
    "vuetify": "^3.3.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vue-tsc": "^1.4.2"
  }
}
